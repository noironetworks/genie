#           super=...]
#
#     relate
#     [
#         name=
#     ]
#     {
#         from
#         [
#             name=class;
#             cardinality=one|many;
#         ]
#
#         to
#         [
#             name=class;
#            cardinality=one|many;
#         ]
#     }
#
#     dependency[name]
#     {
#         from[
#             class;
#             cardinality=one|many;
#             ]
#
#         to[class;
#            cardinality=one|many;
#            ]
