metadata
{
    ref[module]
    {
        ref[class]
        {
            node[contained;
                 use=module/contained;
                 inherit-props=no;
                 ]
            {
                node[parent;
                     use=module/contained/parent;
                     ]
            }
            node[root;
                 use=module/root;
                 inherit-props=no]
        }

        node[root;
             use=root
             ]


        node[contained;
             namespace=containment;
             use=contained
             ]
        {
            node[parent;
                 namespace=containment;
                 use=contained/parent]
        }
    }

    node[root;
         use=contained;
         inherit-props=no
         ]
    {
        prop[class]
    }

    node[contained;
         namespace=containment;
         ]
    {
        prop[class]
        node[parent;
             namespace=containment;]
        {
           prop[class]
           # access rules
           # hide properties
           # ...
        }
    }
}