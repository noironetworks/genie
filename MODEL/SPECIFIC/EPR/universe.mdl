module[epr]
{
    class[Universe;
          abstract;
          ]

    {
        root
    }

    class[L2Universe;
          super=epr/Universe;
          concrete;
          ]
    {
        named
        {
            parent[class=*;]
            {
                component[prefix=l2;]
            }
        }
    }

    class [L3Universe;
           super=epr/Universe;
           concrete;
           ]
    {
        named
        {
            parent[class=*;]
            {
                component[prefix=l3;]
            }
        }
    }

}
